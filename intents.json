{
  "intents": [
    {
      "tag": "greeting",
      "patterns": ["Hi", "Hello", "Hey", "Good morning", "Good evening"],
      "responses": ["Hello!", "Hi there!", "Hey! How can I help?"]
    },
    {
      "tag": "goodbye",
      "patterns": ["Bye", "See you later", "Goodbye", "Catch you later"],
      "responses": ["Goodbye!", "See you later!", "Have a nice day!"]
    },
    {
      "tag": "thanks",
      "patterns": ["Thanks", "Thank you", "Thanks a lot", "Thank you so much"],
      "responses": ["You're welcome!", "No problem!", "Anytime!"]
    },
    {
      "tag": "bot_name",
      "patterns": ["What is your name?", "Who are you?", "Your name?"],
      "responses": ["I am your friendly chatbot.", "You can call me Chatbot!"]
    },
    {
      "tag": "help",
      "patterns": ["Can you help me?", "I need help", "Help me please"],
      "responses": ["Of course! How can I assist you?", "Sure, what do you need help with?"]
    },
    {
      "tag": "age",
      "patterns": ["How old are you?", "What is your age?"],
      "responses": ["I’m ageless!", "I exist outside of time."]
    },
    {
      "tag": "weather",
      "patterns": ["What's the weather?", "Is it raining?", "Tell me the forecast"],
      "responses": ["I’m not connected to live data yet, but I hope it’s sunny!", "Check your local weather app for real-time info."]
    },
    {
      "tag": "joke",
      "patterns": ["Tell me a joke", "Make me laugh", "Do you know any jokes?"],
      "responses": ["Why did the computer go to the doctor? It caught a virus!", "I would tell you a joke about UDP, but you might not get it."]
    },
    {
      "tag": "love",
      "patterns": ["I love you", "Do you love me?", "Love?"],
      "responses": ["I’m flattered!", "I’m just a chatbot, but I appreciate you!"]
    },
    {
      "tag": "food",
      "patterns": ["What should I eat?", "I am hungry", "Suggest a meal"],
      "responses": ["How about some pizza?", "Maybe try a healthy salad today!"]
    }
  ]
}